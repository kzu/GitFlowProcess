<configuration>
    <appSettings>
        <!-- Optionally provides a token via configuration rather than a console parameter. -->
        <add key="apiToken" value="" />
    </appSettings>
    <system.diagnostics>
        <sources>
            <source name="*" switchValue="Information">
                <listeners>
                    <add name="console"
                         type="OctoFlow.SimpleConsoleListener, OctoFlow" />
                </listeners>
            </source>
        </sources>
        <trace autoflush="true" />
    </system.diagnostics>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
